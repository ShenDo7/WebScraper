<ul>
    {{#each saved}}
    <div class="container-fluid article-container">
        <div class="card">
            <div class="card-header row">
                <div class="col-lg-10"><a class="article-link" target="_blank" rel="noopener noreferrer"
                        href={{url}}>{{title}}</a></div>
